<resources>

    <!-- Base application theme. -->
    <style name="AppTheme" parent="Theme.AppCompat.Light.DarkActionBar">
        <!-- Customize your theme here. -->
        <item name="colorPrimary">@color/colorPrimary</item>
        <item name="colorPrimaryDark">@color/colorPrimaryDark</item>
        <item name="colorAccent">@color/colorAccent</item>
    </style>

    <style name="boton">
        <item name="android:background">@drawable/figuraboton</item>
    </style>
    <style name="boton2">
        <item name="android:background">@drawable/figuraboton2</item>
    </style>
    <style name="botoninicio">
        <item name="android:background">@drawable/figurabotoninicio</item>
    </style>
    <style name="layout">
        <item name="android:background">@drawable/figuralayout</item>
        <item name="android:layout_marginBottom">10dp</item>
        <item name="android:layout_marginLeft">10dp</item>
        <item name="android:layout_marginRight">10dp</item>
        <item name="android:paddingBottom">10dp</item>
        <item name="android:paddingLeft">10dp</item>
        <item name="android:paddingRight">10dp</item>
    </style>

</resources>
